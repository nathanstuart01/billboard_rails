<h1>Add New Song</h1>

<%= form_for [@billboard, @song] do |f| %>
<%= f.label :song_name %>
<%= f.text_field :song_name %>
<%= f.label :artist_name %>
<%= f.text_field :artist_name %>
<%= f.submit :Add %>
<% end %>

<nav id="exit">Click to display home links</nav>
  <div id="links">
    <%= link_to 'Back to Song List', billboard_songs_path %>
    <br />
    <%= link_to 'Back to Billboard List', :root %>
  </div>
